{
  "name": "@phx/common",
  "version": "1.0.0",
  "private": true,
  "description": "Prescryptive Experiences Common UX",
  "author": "Prescryptive Health, Inc",
  "license": "UNLICENSED",
  "main": "tslint.json",
  "scripts": {
    "------------------primary commands---------------------------": "",
    "test": "yarn compile && jest --coverage",
    "test:clear": "jest --clearCache",
    "test:watch": "yarn compile && jest --watch",
    "------------------ci commands----------------------------": "",
    "test:ci": "yarn compile && jest -ci --verbose --runInBand",
    "------------------support commands---------------------------": "",
    "vsts-npm-auth": "npx vsts-npm-auth -config .npmrc",
    "compile": "yarn lint && yarn tsc",
    "lint": "yarn eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:errors": "yarn lint --quiet",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:serve": "yarn storybook:build && npx serve ./storybook-static"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@react-native-picker/picker": "2.2.1",
    "@react-navigation/native": "^6.0.6",
    "awesome-debounce-promise": "^2.1.0",
    "cross-env": "^7.0.3",
    "expo-linear-gradient": "^11.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.35",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-calendars": "1.1276.0",
    "react-native-htmlview": "^0.16.0",
    "react-native-markdown-display": "^6.1.6",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-skeleton-content": "^1.0.24",
    "react-native-web": "0.17.1",
    "react-redux": "^7.2.6",
    "react-text-mask": "^5.4.3",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "swiper": "^9.0.5",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-htmlview": "^0.16.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-text-mask": "^5.4.11",
    "@types/xdate": "^0.8.32",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-tailcall-optimization": "^2.0.0",
    "core-js": "^3.21.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.7",
    "jest": "^26.6.2",
    "jest-expo": "^43.0.1",
    "jest-localstorage-mock": "^2.4.18",
    "react-app-polyfill": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.2",
    "whatwg-fetch": "^3.6.2"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
