{
  "name": "prescriptive-ux-guest-member-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "private": true,
  "scripts": {
    "------------------primary commands---------------------------": "",
    "start": "yarn build && yarn serve",
    "start:watch": "yarn build & yarn serve:watch",
    "serve": "node ./build/index.js",
    "serve:watch": "nodemon ./build/index.js",
    "serve:debug": "../../node_modules/.bin/ts-node ./src/index.ts",
    "test": "jest -u --verbose --runInBand --logHeapUsage",
    "test:watch": "jest -u --verbose --watch",
    "build": "yarn build:ci --verbose",
    "build:watch": "yarn build --watch",
    "------------------ci commands--------------------------------": "",
    "build:ci": "yarn compile && webpack --progress --loglevel notice",
    "test:ci": "jest -ci --verbose -runInBand --logHeapUsage",
    "------------------support commands---------------------------": "",
    "vsts-npm-auth": "npx vsts-npm-auth -config .npmrc",
    "compile": "yarn lint && yarn tsc",
    "compile:watch": "yarn compile --watch",
    "lint": "yarn eslint . ../common/src --ext .ts --max-warnings 0",
    "lint:errors": "yarn lint --quiet",
    "------------------docker commands (bash / wsl)---------------": "",
    "docker:all": "docker-compose --env-file ./env.booking-service.test.md up",
    "docker:booking": "docker-compose --env-file ./env.booking-service.test.md up booking",
    "docker:api": "docker-compose --env-file ./env.booking-service.test.md up uxapi",
    "docker:recreate": "yarn docker:all --build --force-recreate"
  },
  "workspaces": {
    "nohoist": [
      "redis",
      "redis/**"
    ]
  },
  "dependencies": {
    "@azure/service-bus": "^1.0.2",
    "@phx/common": "1.0.0",
    "@sendgrid/mail": "7.4.2",
    "applicationinsights": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "brfs": "^2.0.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-validator": "^6.2.0",
    "file-loader": "2.0.0",
    "geolib": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "license-webpack-plugin": "^2.2.0",
    "moment-timezone": "^0.5.35",
    "mongodb-client-encryption": "^2.0.0",
    "mongoose": "^6.5.4",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "node-webtokens": "^1.0.0",
    "path": "^0.12.7",
    "pdfmake": "^0.1.72",
    "redis": "^3.1.1",
    "string-replace-webpack-plugin": "0.1.3",
    "stripe": "^8.93.0",
    "transform-loader": "^0.2.4",
    "twilio": "^3.33.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dateformat": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.42",
    "@types/node-fetch": "^2.5.6",
    "@types/pdfmake": "^0.1.20",
    "@types/redis": "^2.8.16",
    "audit-ci": "^6.2.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "jest-trx-results-processor": "^2.2.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^7.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-security": "^1.16.0",
    "typescript": "^4.6.2"
  }
}